<h1>Listing participantes</h1>

<table>
  <thead>
    <tr>
      <th>Cc</th>
      <th>Primer nombre</th>
      <th>Segundo nombre</th>
      <th>Primer apellido</th>
      <th>Segundo apellido</th>
      <th>Eps</th>
      <th>Ciudad</th>
      <th>Telefono</th>
      <th>Categoria</th>
      <th>Edad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @participantes.each do |participante| %>
      <tr>
        <td><%= participante.cc %></td>
        <td><%= participante.primer_Nombre %></td>
        <td><%= participante.segundo_Nombre %></td>
        <td><%= participante.primer_Apellido %></td>
        <td><%= participante.segundo_Apellido %></td>
        <td><%= participante.eps %></td>
        <td><%= participante.ciudad %></td>
        <td><%= participante.telefono %></td>
        <td><%= participante.categoria %></td>
        <td><%= participante.edad %></td>
        <td><%= link_to 'Show', participante %></td>
        <td><%= link_to 'Edit', edit_participante_path(participante) %></td>
        <td><%= link_to 'Destroy', participante, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Participante', new_participante_path %>
